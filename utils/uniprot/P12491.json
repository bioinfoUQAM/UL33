 subtypes (A to D, F to H, J and K)"}],"commentType":"MISCELLANEOUS"}],"features":[{"type":"Signal","location":{"start":{"value":1,"modifier":"EXACT"},"end":{"value":31,"modifier":"EXACT"}},"description":"","evidences":[{"evidenceCode":"ECO:0000250"}]},{"type":"Chain","location":{"start":{"value":32,"modifier":"EXACT"},"end":{"value":460,"modifier":"EXACT"}},"description":"Truncated surface protein","featureId":"PRO_0000038379"},{"type":"Region","location":{"start":{"value":130,"modifier":"EXACT"},"end":{"value":148,"modifier":"EXACT"}},"description":"V1"},{"type":"Region","location":{"start":{"value":149,"modifier":"EXACT"},"end":{"value":193,"modifier":"EXACT"}},"description":"V2"},{"type":"Region","location":{"start":{"value":293,"modifier":"EXACT"},"end":{"value":325,"modifier":"EXACT"}},"description":"V3"},{"type":"Region","location":{"start":{"value":358,"modifier":"EXACT"},"end":{"value":368,"modifier":"EXACT"}},"description":"CD4-binding loop","evidences":[{"evidenceCode":"ECO:0000250"}]},{"type":"Region","location":{"start":{"value":379,"modifier":"EXACT"},"end":{"value":412,"modifier":"EXACT"}},"description":"V4"},{"type":"Glycosylation","location":{"start":{"value":87,"modifier":"EXACT"},"end":{"value":87,"modifier":"EXACT"}},"description":"N-linked (GlcNAc...) asparagine; by host","evidences":[{"evidenceCode":"ECO:0000255"}],"featureId":""},{"type":"Glycosylation","location":{"start":{"value":135,"modifier":"EXACT"},"end":{"value":135,"modifier":"EXACT"}},"description":"N-linked (GlcNAc...) asparagine; by host","evidences":[{"evidenceCode":"ECO:0000255"}],"featureId":""},{"type":"Glycosylation","location":{"start":{"value":139,"modifier":"EXACT"},"end":{"value":139,"modifier":"EXACT"}},"description":"N-linked (GlcNAc...) asparagine; by host","evidences":[{"evidenceCode":"ECO:0000255"}],"featureId":""},{"type":"Glycosylation","location":{"start":{"value":148,"modifier":"EXACT"},"end":{"value":148,"modifier":"EXACT"}},"description":"N-linked (GlcNAc...) as